abstract class C  {
    private _x = 10;
    public get x(): __UnknownType__ {
        return this._x;
    }
    public set x(x : __UnknownType__ /* number|string */){
        if (__untranslated_expression( /* typeof x */) === "string") this._x = x.length;
        else this._x = x;
    }
    public abstract get z(): Double ;
    public abstract set z(n : Double);
}

open class D extends C  {
    public override get x(): Double {
        return 100;
    }
    public static get y(): String {
        return "blah";
    }
    public get z(): Double {
        return this.x;
    }
    public set z(n : Double){
        this.z = n;
    }
}

