open class C  {
    public f : Double = 1;
    public e : String = "C";
    public foo(): void {
        console.log("class C");
    }
}

let c1 : C = new C(2, "global") {
    public foo(): void {
        console.log("global");
    }
    public constructor(f : Double, e : String) {
        super();
        this.f = f;
        this.e = e;
    }

};
let c2 : C = new C(3, "global") {
    public get e(): String {
        return this.e;
    }
    public constructor(f : Double, e : String) {
        super();
        this.f = f;
        this.e = e;
    }

};
function bar(c : C): void {
    c.foo();
}
function zoo(): void {
    let e = "zoo";
    bar(new C(3, e) {
        public foo(): void {
            console.log("zoo");
        }
        public constructor(f : Double, e : String) {
            super();
            this.f = f;
            this.e = e;
        }

    });
}
function main(): void {
    bar(c);
    zoo();
}
