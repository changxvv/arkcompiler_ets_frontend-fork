open class A  {
    private num : Double ;
    protected str : String = "A";
    public constructor(n : Double) {
        this.num = n;
    }

    protected foo(): Double {
        return this.num * this.num;
    }
}

open class B extends A  {
    private id : String ;
    public constructor(n : Double, s : String) {
        super(n);
        this.id = s;
    }

    public foo(): Double {
        let slen = this.str.length;
        return super.foo() + slen * slen;
    }
    private getId(): String {
        return "B" + this.id;
    }
    public bar(): String {
        return super.str + this.getId();
    }
    public goo(): String {
        return this.id + super.str;
    }
}

