package com.ohos.migrator.test.java;

import * from "std/math/math";
open class A  {
    a : int = 1;
    b : int = 3;
    public constructor() {
        a = 2;
        b = 4;
// Inits are inserted here.
        a = 10;
        b = 20;
    }

    public constructor(p : int) {
        // Inits are NOT inserted here.
this();
        a = 30;
        b = 40;
    }

    public constructor(s : String) {
        a = 2;
        b = 4;
// Inits are inserted here.
        a = 50;
        b = 60;
    }

}

open class B extends A  {
    c : int = 11;
    d : int = 33;
    public constructor() {
        c += 22;
        d -= 44;
// Inits are inserted here.
        c = 100;
        d = 200;
    }

    public constructor(p : int) {
        // Inits are NOT inserted here.
this();
        c = 300;
        d = 400;
    }

    public constructor(s : String) {
        super(0);
        c += 22;
        d -= 44;
// Inits are inserted here (after super() call).
        c = 500;
        d = 600;
    }

}

// instance initializer without ctors
open class C  {
    foo : String ;
    public constructor() {
        foo = "bar";
    }

}

